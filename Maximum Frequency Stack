class FreqStack {
public:
    unordered_map<int,int> freq;
    unordered_map<int,stack<int>> m;
    int mxFreq;
    FreqStack() {
        mxFreq=0;
    }
    
    void push(int x) {
        freq[x]++;
        mxFreq=max(mxFreq,freq[x]);
        m[freq[x]].push(x);
    }
    
    int pop() {
        int ans=m[mxFreq].top();m[mxFreq].pop();
        freq[ans]--;
        if(m[mxFreq].size()==0){
            m.erase(mxFreq);
            mxFreq--;
        }
        return ans;
    }
};
